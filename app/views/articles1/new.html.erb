<h1>Create New Article</h1>

<%= form_for @articles1 do |f| %>
    
    <p>
        <%= f.lable :title %><br />
        <%= f.text_field :title %>
    </p>
    <p>
        <%= f.lable :description %><br />
        <%= f.text_area :description %>
    </p>
    <p>
        <%= f.submit %>
    </p>
<% end %>